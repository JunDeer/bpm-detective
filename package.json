  "bugs": {
    "url": "https://github.com/tornqvist/bpm-detective/issues"
  },
  "homepage": "https://github.com/tornqvist/bpm-detective#readme",
  "devDependencies": {
    "babel-eslint": "^4.0.7",
    "babelify": "^6.2.0",
    "brfs": "^1.4.1",
    "browserify": "^11.0.1",
    "eslint": "^1.0.0",
    "serve": "^1.4.0",
    "tape": "^4.0.1",
    "tape-run": "^1.1.0",
    "whatwg-fetch": "^0.9.0"
  },
  "dependencies": {
    "babel": "^5.8.23"
